{
    "name": "uspdev/toolkit",
    "type": "project",
    "description": "Kit de ferramentas de testes para bibliotecas USPDev",
    "keywords": [
        "toolkit",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^11",
        "laravel/tinker": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.2",
        "uspdev/laravel-replicado": "dev-main",
        "uspdev/laravel-tools": "dev-main",
        "uspdev/laravel-usp-theme": "dev-master",
        "uspdev/replicado": "dev-master",
        "uspdev/senhaunica-socialite": "dev-master",
        "uspdev/utils": "dev-master",
        "uspdev/wsfoto": "dev-master",
        "uspdev/forms": "dev-main"
    },
    "repositories": [
        {
            "type": "path",
            "url": "uspdev/laravel-tools"
        },
        {
            "type": "path",
            "url": "uspdev/laravel-usp-theme"
        },
        {
            "type": "path",
            "url": "uspdev/laravel-replicado"
        },
        {
            "type": "path",
            "url": "uspdev/replicado"
        },
        {
            "type": "path",
            "url": "uspdev/senhaunica-socialite"
        },
        {
            "type": "path",
            "url": "uspdev/utils"
        },
        {
            "type": "path",
            "url": "uspdev/forms"
        },
        {
            "type": "path",
            "url": "uspdev/wsfoto"
        }
    ],
    "require-dev": {
        "spatie/laravel-ignition": "*",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-autoload-dump": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --provider=\"Uspdev\\UspTheme\\ServiceProvider\" --tag=assets --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
